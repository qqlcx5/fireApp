
.list {
  min-height: 200rpx;
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 28rpx;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  padding: 30rpx 30rpx 0;
  box-sizing: border-box;
}
.item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  margin-bottom: 30rpx;
  position: relative;
  width: 690rpx;
  height: 180rpx;
  background-color: #ffffff;
  box-shadow: 0 3rpx 5rpx 0 #e6e6e6;
  position: relative;
}
.item-left {
  height: 180rpx;
  width: 180rpx;
  padding: 30rpx;
  box-sizing: border-box;
  -webkit-flex-shrink: 0;
  flex-shrink: 0;
}
.item-left-image {
  width: 100%;
  height: 100%;
  border-radius: 12rpx;
}
.item-middle {
  width: 360rpx;
  height: 180rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  overflow: hidden;
}
.item-name {
  width: 360rpx;
  height: 50rpx;
  font-size: 36rpx;
  color: #333333;
  margin: 25rpx 0 14rpx;
  overflow: hidden;
}
.item-desc {
  font-size: 24rpx;
  line-height: 40rpx;
  color: #666666;
}
.item-right {
  width: 150rpx;
  height: 180rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.item-right-image {
  width: 60rpx;
  height: 60rpx;
  margin: 20rpx;
}
.item-btn-warn {
  width: 96rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  border-radius: 10rpx;
  border: solid 1rpx #d0021b;
  font-size: 28rpx;
  color: #d0021b;
}
.item-btn-info {
  width: 96rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  background-color: #ffffff;
  border-radius: 10rpx;
  border: solid 1rpx #d0021b;
  font-size: 28rpx;
  color: #d0021b;
}
.msg-box {
  min-height: 100rpx;
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  margin-top: 300rpx;
}
.msg-img {
  width: 160rpx;
  height: 160rpx;
  margin-bottom: 40rpx;
}
.msg-title {
  color: #999;
}
.load-more {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  height: 80rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}
.loading-img {
  height: 24rpx;
  width: 24rpx;
  margin-right: 10rpx;
}
.loading-text {
  font-size: 28rpx;
  color: #777777;
}
.loading-img>view {
  position: absolute;
}
.load1,
.load2,
.load3 {
  height: 24rpx;
  width: 24rpx;
}
.load2 {
  -webkit-transform: rotate(30deg);
  transform: rotate(30deg);
}
.load3 {
  -webkit-transform: rotate(60deg);
  transform: rotate(60deg);
}
.loading-img>view view {
  width: 6rpx;
  height: 2rpx;
  border-top-left-radius: 1rpx;
  border-bottom-left-radius: 1rpx;
  background: #777;
  position: absolute;
  opacity: 0.2;
  -webkit-transform-origin: 50%;
  transform-origin: 50%;
  -webkit-animation: load 1.56s ease infinite;
}
.loading-img>view view:nth-child(1) {
  -webkit-transform: rotate(90deg);
  transform: rotate(90deg);
  top: 2rpx;
  left: 9rpx;
}
.loading-img>view view:nth-child(2) {
  -webkit-transform: rotate(180deg);
  top: 11rpx;
  right: 0;
}
.loading-img>view view:nth-child(3) {
  -webkit-transform: rotate(270deg);
  transform: rotate(270deg);
  bottom: 2rpx;
  left: 9rpx;
}
.loading-img>view view:nth-child(4) {
  top: 11rpx;
  left: 0;
}
.load1 view:nth-child(1) {
  -webkit-animation-delay: 0s;
  animation-delay: 0s;
}
.load2 view:nth-child(1) {
  -webkit-animation-delay: 0.13s;
  animation-delay: 0.13s;
}
.load3 view:nth-child(1) {
  -webkit-animation-delay: 0.26s;
  animation-delay: 0.26s;
}
.load1 view:nth-child(2) {
  -webkit-animation-delay: 0.39s;
  animation-delay: 0.39s;
}
.load2 view:nth-child(2) {
  -webkit-animation-delay: 0.52s;
  animation-delay: 0.52s;
}
.load3 view:nth-child(2) {
  -webkit-animation-delay: 0.65s;
  animation-delay: 0.65s;
}
.load1 view:nth-child(3) {
  -webkit-animation-delay: 0.78s;
  animation-delay: 0.78s;
}
.load2 view:nth-child(3) {
  -webkit-animation-delay: 0.91s;
  animation-delay: 0.91s;
}
.load3 view:nth-child(3) {
  -webkit-animation-delay: 1.04s;
  animation-delay: 1.04s;
}
.load1 view:nth-child(4) {
  -webkit-animation-delay: 1.17s;
  animation-delay: 1.17s;
}
.load2 view:nth-child(4) {
  -webkit-animation-delay: 1.30s;
  animation-delay: 1.30s;
}
.load3 view:nth-child(4) {
  -webkit-animation-delay: 1.43s;
  animation-delay: 1.43s;
}
@-webkit-keyframes load {
0% {
    opacity: 1;
}
100% {
    opacity: 0.2;
}
}