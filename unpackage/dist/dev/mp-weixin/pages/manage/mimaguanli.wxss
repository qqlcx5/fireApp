
.list {
  min-height: 110rpx;
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.item {
  width: 750rpx;
  height: 110rpx;
  border-bottom: 2rpx solid #e6e6e6;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  font-size: 32rpx;
  color: #6c6c6c;
}
.item-name {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.desc {
  width: 750rpx;
  min-height: 0;
  margin: 20rpx 0 40rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.desc-text {
  font-size: 24rpx;
  color: #999999;
  text-align: justify;
}
.keep {
  width: 750rpx;
  min-height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}