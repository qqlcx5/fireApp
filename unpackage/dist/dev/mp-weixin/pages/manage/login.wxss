
.page {
  height: 100vh;
  width: 750rpx;
  background-color: #fff;
}
.banner {
  width: 750rpx;
  height: 386rpx;
  overflow: hidden;
}
.banner image {
  width: 100%;
  height: 100%;
}
.list {
  min-height: 110rpx;
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  margin-top: 30rpx;
}
.item {
  width: 750rpx;
  height: 110rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  font-size: 32rpx;
  color: #6c6c6c;
  border-bottom: solid 2rpx #e6e6e6;
}
.item-name {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.keep {
  width: 750rpx;
  min-height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  margin-top: 80rpx;
}