
.list {
  min-height: 110rpx;
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  margin: 100rpx 0 0;
}
.item {
  width: 690rpx;
  height: 110rpx;
  margin: 40rpx auto 0;
  border-bottom: 2rpx solid #e6e6e6;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  font-size: 32rpx;
  color: #6c6c6c;
  border-radius: 50rpx;
}
.item-name {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.keep {
  width: 750rpx;
  min-height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  margin-top: 100rpx;
}