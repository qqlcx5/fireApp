
.list {
  min-height: 200rpx;
  width: 710rpx;
  margin: 20rpx;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 28rpx;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.item-user {
  height: 140rpx;
  width: 710rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.item-user-name {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  font-size: 32rpx;
  color: #666666;
}
.item-user-border {
  height: 140rpx;
  width: 710rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  border-bottom: 2rpx solid #eee;
}
.item-user-img {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  background-color: #d8d8d8;
}
.item {
  height: 88rpx;
  width: 710rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.item .border {
  height: 88rpx;
  width: 630rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  border-bottom: 2rpx solid #eee;
}
.picker-item {
  width: 630rpx;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 32rpx;
  color: #999999;
}
.picker-name {
  min-width: 60rpx;
  font-size: 32rpx;
  color: #666666;
  margin-right: 60rpx;
}
.item-name {
  min-width: 120rpx;
  font-size: 32rpx;
  color: #666666;
}
.item-input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  text-align: center;
  height: 88rpx;
}
.account {
  width: 750rpx;
  min-height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}