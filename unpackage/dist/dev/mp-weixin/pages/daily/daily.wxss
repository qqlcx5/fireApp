
.banner {
  height: 390rpx;
  width: 750rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.banner image {
  width: 100%;
  height: 100%;
}
.list {
  min-height: 100rpx;
  width: 720rpx;
  overflow: hidden;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
  margin-left: 30rpx;
}
.item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  margin: 0 30rpx 30rpx 0;
  width: 210rpx;
  height: 210rpx;
  background-color: #ffffff;
  border-radius: 6rpx;
}
.item image {
  width: 75rpx;
  height: 75rpx;
  border-radius: 12rpx;
}
.item-name {
  margin-top: 10rpx;
  font-size: 28rpx;
  color: #666666;
}
.item .imglogo {
  width: 210rpx;
  height: 210rpx;
}
.notify {
  height: 100rpx;
  width: 750rpx;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  font-size: 28rpx;
  color: #666666;
  border-bottom: 2rpx solid #e9e9e9;
}
.notifyIcon {
  color: #d0021b;
}