
page {
  min-height: 100vh;
  width: 100%;
  font-size: 32rpx;
  background-color: #f7f7f7;
  overflow-x: hidden;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.mgr20 {
  margin-right: 20rpx;
}
.mgl20 {
  margin-left: 20rpx;
}
.mgr30 {
  margin-right: 30rpx;
}
.mgl30 {
  margin-left: 30rpx;
}
.mgt20 {
  margin-top: 20rpx;
}
@font-face {
  font-family: iconfont;
  font-weight: normal;
  font-style: normal;
  src: url('https://at.alicdn.com/t/font_874003_cx48blm2ceq.ttf')
    format('truetype');
}
.icon {
  font-size: 56rpx;
  color: #fff;
  font-family: iconfont;
}
.gIcon {
  font-size: 48rpx;
  color: #dadada;
  font-family: iconfont;
}