
.header {
  height: 100rpx;
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  padding-top: 20rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  background-color: #fff;
  box-shadow: 0 4rpx 8rpx 0 rgba(238, 238, 238, 0.5);
}
.header-left,
.header-right {
  width: 80rpx;
  height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}
.header-content {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  font-size: 36rpx;
  text-align: center;
}
.banner {
  height: 360rpx;
  width: 750rpx;
  overflow: hidden;
}
.banner image {
  width: 100%;
  height: 100%;
}
.notify {
  height: 100rpx;
  width: 750rpx;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.list {
  min-width: 100rpx;
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
}
.item {
  width: 373rpx;
  height: 140rpx;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  margin: 0 4rpx 4rpx 0;
}
.item image {
  width: 80rpx;
  height: 80rpx;
}
.item-name {
  width: 140rpx;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #eee;
}