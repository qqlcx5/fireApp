
.header-right text {
  font-size: 28rpx;
  color: #ffffff;
}
.list {
  min-height: 110rpx;
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.item-time {
  height: 84rpx;
  width: 750rpx;
  line-height: 84rpx;
  font-size: 24rpx;
  color: #999999;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.item {
  width: 750rpx;
  height: 174rpx;
  background-color: #ffffff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.item-left {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  height: 174rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}
.item-left text:nth-of-type(1) {
  font-size: 32rpx;
  color: #666666;
  font-weight: bold;
}
.item-left text:nth-of-type(2) {
  margin-top: 15rpx;
  font-size: 24rpx;
  color: #999999;
}
.item-right {
  width: 220rpx;
  height: 174rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
  align-items: flex-end;
}
.state-color {
  font-size: 28rpx;
  color: #e30000;
}
.push-color {
  color: #67ba61;
}
.btn-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.mbtn {
  width: 100rpx;
  height: 54rpx;
}