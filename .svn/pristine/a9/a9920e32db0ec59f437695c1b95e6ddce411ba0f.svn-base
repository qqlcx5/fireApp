.sele-title{width:100%;height:80rpx;display:-webkit-box;display:-webkit-flex;display:flex;overflow-x:auto;overflow-y:hidden}.sele-item{-webkit-box-flex:1;-webkit-flex:1;flex:1;height:80rpx;display:block;background-color:#fff;padding:0 10rpx;box-sizing:border-box;line-height:80rpx;text-align:center;color:#666}.sele-item.select{border-bottom:2rpx solid #d0021b}.list{min-height:200rpx;font-size:28rpx;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column}.item,.list{width:750rpx;display:-webkit-box;display:-webkit-flex;display:flex}.item{height:200rpx;background-color:#fff;padding:30rpx;box-sizing:border-box;border-bottom:2rpx solid #e6e6e6;position:relative}.item-left image{width:140rpx;height:140rpx;background-color:#d8d8d8}.item-right{width:520rpx;height:140rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column}.item-name{font-size:32rpx;color:#666;font-weight:700;white-space:nowrap}.item-desc,.item-name{overflow:hidden;text-overflow:ellipsis}.item-desc{margin-top:20rpx;font-size:24rpx;color:#999;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical}.item-time{position:absolute;bottom:10rpx;right:30rpx;font-size:20rpx;color:#dadada}