
.tab-title {
  width: 750rpx;
  height: 70rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  margin: 20rpx 0;
}
.tab-title .tab {
  width: 280rpx;
  height: 70rpx;
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: inline-flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  border: solid 2rpx #e45252;
  box-sizing: border-box;
  color: #666666;
}
.tab-title .select {
  background-color: #d0021b;
  color: #ffffff;
}
.tab-title .tab:nth-child(1) {
  border-radius: 12rpx 0 0 12rpx;
}
.tab-title .tab:nth-child(2) {
  border-radius: 0 12rpx 12rpx 0;
}
.week-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 750rpx;
  min-height: 730rpx;
  margin-top: 60rpx;
}
.week-left {
  width: 130rpx;
  min-height: 730rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.week-left-item {
  width: 130rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  background-color: #c12158;
  border-radius: 0 35rpx 35rpx 0;
  margin-bottom: 40rpx;
  color: #ffffff;
}
.week-left .select {
  background-color: #e45252;
}
.week-right {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  min-height: 730rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
  padding: 0 20rpx 0 30rpx;
  box-sizing: border-box;
}
.week-right-time {
  width: 180rpx;
  height: 134rpx;
  background-color: #ffffff;
  box-shadow: 0 0 8rpx 0 #e6e6e6;
  border-radius: 12rpx;
  margin: 0 10rpx 10rpx 0;
  font-size: 28rpx;
  color: #666666;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  overflow: hidden;
}
.week-right-time-top {
  width: 180rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  font-size: 24rpx;
  background-color: #ffffff;
}
.week-right-time-bottom {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  width: 180rpx;
  background-color: #d0021b;
  font-size: 26rpx;
  color: #ffffff;
}
.week-right-item {
  width: 180rpx;
  height: 134rpx;
  background-color: #ffffff;
  box-shadow: 0 0 8rpx 0 #e6e6e6;
  border-radius: 12rpx;
  margin: 0 10rpx 10rpx 0;
  font-size: 28rpx;
  color: #666666;
  line-height: 134rpx;
  text-align: center;
}