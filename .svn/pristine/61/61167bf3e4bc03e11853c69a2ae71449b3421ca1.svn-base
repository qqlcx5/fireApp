
.info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  width: 750rpx;
  min-height: 0;
}
.info-title {
  width: 750rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  color: #333333;
  font-weight: bold;
}
.info-msg {
  width: 750rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  text-align: justify;
  font-size: 24rpx;
  line-height: 40rpx;
  color: #666666;
}