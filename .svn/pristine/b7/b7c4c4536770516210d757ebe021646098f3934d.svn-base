.header{height:128rpx;width:100%;padding-top:40rpx;box-sizing:border-box;padding-top:20rpx;background-color:#d0021b;box-shadow:0 4rpx 8rpx 0 #e6e6e6}.header,.header-left,.header-right{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center}.header-left,.header-right{width:88rpx;height:88rpx;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center}.header-content{-webkit-box-flex:1;-webkit-flex:1;flex:1;text-align:center;font-size:36rpx;color:#fff}.list{-webkit-flex-wrap:wrap;flex-wrap:wrap}.item,.list{min-height:240rpx;width:750rpx;display:-webkit-box;display:-webkit-flex;display:flex}.item{-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;padding:0 30rpx;box-sizing:border-box}.item-time{height:60rpx;width:690rpx;text-align:center;line-height:60rpx;font-size:24rpx;color:#999}.item-info{width:690rpx;min-height:200rpx;background-color:#fff;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;border-radius:10rpx}.item-name{margin:20rpx;font-size:32rpx;color:#666;font-weight:700;white-space:nowrap}.item-desc,.item-name{overflow:hidden;text-overflow:ellipsis}.item-desc{margin:0 20rpx 20rpx;font-size:26rpx;color:#999;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical}.item-info-detail{width:690rpx;height:80rpx;border-top:2rpx solid #e6e6e6;line-height:80rpx;text-align:right;font-size:26rpx;color:#4a90e2;padding-right:20rpx;box-sizing:border-box}