
.list {
  min-height: 0;
  width: 710rpx;
  margin: 20rpx;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 28rpx;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.item {
  height: 88rpx;
  width: 710rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.item .border {
  height: 88rpx;
  width: 630rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  border-bottom: 2rpx solid #eee;
}
.picker-item {
  width: 630rpx;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 32rpx;
  color: #999999;
}
.picker-name {
  min-width: 60rpx;
  font-size: 32rpx;
  color: #666666;
  margin-right: 60rpx;
}
.item-textarea {
  height: 200rpx;
  width: 710rpx;
  padding: 30rpx 40rpx;
  box-sizing: border-box;
}
.item-textarea ._textarea {
  width: 100%;
  height: 100%;
}
.picture {
  min-height: 160rpx;
  width: 710rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.picture-name {
  height: 60rpx;
  line-height: 60rpx;
  width: 630rpx;
  font-size: 32rpx;
  color: #333333;
}
.addIcon {
  margin-top: 10rpx;
  font-size: 68rpx;
  color: #dadada;
  font-family: iconfont;
}
.submit {
  height: 88rpx;
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  margin-top: 100rpx;
}
.submit .btn {
  background-color: #67ba61;
}