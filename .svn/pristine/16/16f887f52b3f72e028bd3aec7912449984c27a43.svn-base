
page {
  min-height: 100vh;
  max-height: 100%;
  width: 100%;
  font-size: 32rpx;
  background-color: #F7F7F7;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.mgr20 {
  margin-right: 20rpx;
}
.mgl20 {
  margin-left: 20rpx;
}
.mgr10 {
  margin-right: 10rpx;
}
.mgl10 {
  margin-left: 10rpx;
}
@font-face {
  font-family: iconfont;
  font-weight: normal;
  font-style: normal;
  src: url('https://at.alicdn.com/t/font_874003_3w8ux5fmx0r.ttf') format('truetype');
}
.icon {
  font-size: 32rpx;
  color: #b6b6b6;
  font-family: iconfont;
}
.iconWhite {
  font-size: 32rpx;
  color: #FFFFFF;
  font-family: iconfont;
}