
page {
  min-height: 100vh;
  max-height: 100%;
  width: 100%;
  font-size: 32rpx;
  background-color: #f7f7f7;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.mgr20 {
  margin-right: 20rpx;
}
.mgl20 {
  margin-left: 20rpx;
}
.mgt20 {
  margin-top: 20rpx;
}
@font-face {
  font-family: iconfont;
  font-weight: normal;
  font-style: normal;
  src: url('https://at.alicdn.com/t/font_874003_o7llb3pv1md.ttf')
    format('truetype');
}
.icon {
  font-size: 54rpx;
  color: #b6b6b6;
  font-family: iconfont;
}
.iconWhite {
  font-size: 62rpx;
  color: #ffffff;
  font-family: iconfont;
}