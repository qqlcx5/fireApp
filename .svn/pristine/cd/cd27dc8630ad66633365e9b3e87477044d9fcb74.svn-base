
.list {
  min-height: 100rpx;
  width: 720rpx;
  margin: 30rpx 0 0 30rpx;
  overflow: hidden;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
}
.item {
  width: 330rpx;
  height: 160rpx;
  background-color: #fff;
  margin: 0 30rpx 30rpx 0;
  border-radius: 12rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
  font-size: 32rpx;
  color: #666666;
  box-shadow: 0 4rpx 8rpx 0 #e6e6e6;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.item image {
  width: 60rpx;
  height: 60rpx;
  margin-right: 10rpx;
}